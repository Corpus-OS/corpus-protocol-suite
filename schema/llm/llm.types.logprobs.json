{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://corpusos.com/schemas/llm/llm.types.logprobs.json",
  "title": "LLM Log Probabilities",
  "description": "Optional log probability information for generated tokens (structure is forward-compatible).",
  "type": "object",
  "properties": {
    "tokens": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "token_logprobs": {
      "type": "array",
      "items": {
        "type": [
          "number",
          "null"
        ]
      }
    },
    "top_logprobs": {
      "type": [
        "array",
        "null"
      ],
      "items": {
        "type": "object",
        "additionalProperties": true
      }
    },
    "text_offset": {
      "type": [
        "array",
        "null"
      ],
      "items": {
        "type": "integer"
      }
    }
  },
  "additionalProperties": true
}
