{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://corpusos.com/schemas/graph/graph.vertex.create.request.json",
  "title": "graph.create_vertex Args",
  "type": "object",
  "additionalProperties": false,
  "required": ["label", "props"],
  "properties": {
    "label": {
      "type": "string",
      "minLength": 1,
      "maxLength": 128,
      "pattern": "^[A-Za-z0-9._:-]{1,128}$"
    },
    "props": {
      "$ref": "https://corpusos.com/schemas/graph/graph.types.entity.json#/properties/props"
    }
  }
}
