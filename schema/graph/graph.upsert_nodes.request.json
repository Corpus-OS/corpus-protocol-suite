{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://corpusos.com/schemas/graph/graph.upsert_nodes.request.json",
  "title": "graph.upsert_nodes Args",
  "type": "object",
  "additionalProperties": false,
  "required": ["nodes"],
  "properties": {
    "nodes": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": ["id", "label", "props"],
        "properties": {
          "id": {
            "$ref": "https://corpusos.com/schemas/graph/graph.types.id.json"
          },
          "label": {
            "type": "string",
            "minLength": 1,
            "maxLength": 128,
            "pattern": "^[A-Za-z0-9._:-]{1,128}$"
          },
          "props": {
            "$ref": "https://corpusos.com/schemas/graph/graph.types.entity.json#/properties/props"
          }
        }
      }
    },
    "namespace": {
      "type": "string",
      "minLength": 1,
      "maxLength": 128
    }
  }
}
