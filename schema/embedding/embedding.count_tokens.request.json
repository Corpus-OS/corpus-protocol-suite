{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://corpusos.com/schemas/embedding/embedding.count_tokens.request.json",
  "title": "embedding.count_tokens Args",
  "description": "CountTokensSpec for embedding.count_tokens per PROTOCOLS.md ยง3.8 and ยง19.4.",
  "type": "object",
  "additionalProperties": false,
  "required": ["text", "model"],
  "properties": {
    "text": {
      "type": "string",
      "minLength": 0,
      "maxLength": 2000000
    },
    "model": {
      "type": "string",
      "minLength": 1,
      "maxLength": 128,
      "pattern": "^[A-Za-z0-9._:-]{1,128}$"
    }
  },
  "patternProperties": {
    "^(vendor:)[a-z0-9_.:-]+$": {
      "type": ["string", "number", "boolean", "object", "array", "null"]
    }
  },
  "$comment": "Shape matches shared CountTokensSpec: { text: string; model: string }."
}
