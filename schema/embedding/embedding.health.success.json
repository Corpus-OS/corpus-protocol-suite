{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://corpusos.com/schemas/embedding/embedding.health.success.json",
  "title": "embedding.health Result",
  "type": "object",
  "additionalProperties": false,
  "required": ["ok", "server", "version"],
  "properties": {
    "ok": {
      "type": "boolean"
    },
    "server": {
      "type": "string",
      "maxLength": 128
    },
    "version": {
      "type": "string",
      "maxLength": 64
    },
    "status": {
      "type": "string",
      "enum": ["degraded", "ok"]
    },
    "message": {
      "type": "string",
      "maxLength": 512
    },
    "models": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "available": {
            "type": "boolean"
          },
          "rate_limited": {
            "type": "boolean"
          },
          "degraded": {
            "type": "boolean"
          }
        }
      }
    },
    "rate_limits": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "tokens_per_minute": {
          "type": "integer",
          "minimum": 0
        },
        "requests_per_minute": {
          "type": "integer",
          "minimum": 0
        }
      }
    }
  },
  "$comment": "EmbeddingHealthStatus per PROTOCOLS.md ยง19.5."
}
