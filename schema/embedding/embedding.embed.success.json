{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://corpusos.com/schemas/embedding/embedding.embed.success.json",
  "title": "embedding.embed Result",
  "type": "object",
  "additionalProperties": false,
  "required": ["embedding", "model", "text"],
  "properties": {
    "embedding": {
      "$ref": "https://corpusos.com/schemas/embedding/embedding.types.vector.json"
    },
    "model": {
      "type": "string",
      "minLength": 1,
      "maxLength": 128
    },
    "text": {
      "type": "string",
      "minLength": 1,
      "maxLength": 2000000
    },
    "tokens_used": {
      "type": "integer",
      "minimum": 0
    },
    "truncated": {
      "type": "boolean"
    }
  },
  "$comment": "EmbedResult per PROTOCOLS.md ยง18.3. 'text' MAY be truncated when 'truncated' is true."
}
