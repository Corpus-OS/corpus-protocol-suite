{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://corpusos.com/schemas/graph/graph.types.node.json",
  "title": "Graph Node",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "minLength": 1
    },
    "labels": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "default": []
    },
    "properties": {
      "type": "object",
      "additionalProperties": true,
      "default": {}
    },
    "namespace": {
      "type": "string"
    },
    "created_at": {
      "type": [
        "integer",
        "null"
      ],
      "minimum": 0
    },
    "updated_at": {
      "type": [
        "integer",
        "null"
      ],
      "minimum": 0
    }
  },
  "required": [
    "id",
    "properties"
  ],
  "additionalProperties": false
}
