{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://corpusos.com/schemas/embedding/embedding.count_tokens.success.json",
  "title": "embedding.count_tokens Result",
  "oneOf": [
    {
      "type": "object",
      "additionalProperties": false,
      "required": ["count", "model"],
      "properties": {
        "count": {
          "type": "integer",
          "minimum": 0
        },
        "model": {
          "type": "string",
          "minLength": 1,
          "maxLength": 128
        },
        "breakdown": {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "system": {"type": "integer", "minimum": 0},
            "user": {"type": "integer", "minimum": 0},
            "assistant": {"type": "integer", "minimum": 0},
            "tool": {"type": "integer", "minimum": 0}
          }
        }
      }
    },
    {
      "type": "object",
      "additionalProperties": false,
      "required": ["counts", "model"],
      "properties": {
        "counts": {
          "type": "array",
          "items": {"type": "integer", "minimum": 0}
        },
        "model": {
          "type": "string",
          "minLength": 1,
          "maxLength": 128
        },
        "breakdowns": {
          "type": "array",
          "items": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
              "system": {"type": "integer", "minimum": 0},
              "user": {"type": "integer", "minimum": 0},
              "assistant": {"type": "integer", "minimum": 0},
              "tool": {"type": "integer", "minimum": 0}
            }
          }
        }
      }
    }
  ]
}
