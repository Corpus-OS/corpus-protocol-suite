{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://corpusos.com/schemas/vector/vector.types.query_result.json",
  "title": "QueryResult",
  "description": "Structured result payload from vector.query per protocol ยง14.4.",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "matches": {
      "type": "array",
      "items": { "$ref": "https://corpusos.com/schemas/vector/vector.types.vector_match.json" }
    },
    "query_vector": {
      "type": "array",
      "items": { "type": "number" }
    },
    "namespace": {
      "type": "string",
      "maxLength": 256,
      "pattern": "^[A-Za-z0-9._:-]{1,256}$"
    },
    "dimensions": { "type": "integer", "minimum": 1 },
    "id": { "type": "string" },
    "metadata": { "type": "object" },
    "total_matches": { "type": "integer", "minimum": 0 }
  },
  "required": ["matches", "query_vector", "namespace", "total_matches"]
}