{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://corpusos.com/schemas/vector/vector.types.vector.json",
  "title": "Vector",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "minLength": 1,
      "pattern": ".*\\S.*"
    },
    "vector": {
      "type": "array",
      "items": {
        "type": "number"
      },
      "minItems": 1
    },
    "metadata": {
      "type": [
        "object",
        "null"
      ],
      "additionalProperties": true
    },
    "namespace": {
      "type": [
        "string",
        "null"
      ]
    },
    "text": {
      "type": [
        "string",
        "null"
      ]
    }
  },
  "required": [
    "id",
    "vector"
  ],
  "additionalProperties": false
}
