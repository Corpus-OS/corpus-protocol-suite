{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://corpusos.com/schemas/graph/graph.types.warning.json",
  "title": "Graph Warning",
  "type": "object",
  "additionalProperties": false,
  "required": ["code", "message"],
  "properties": {
    "code": {
      "type": "string",
      "pattern": "^[A-Z0-9_]{1,64}$"
    },
    "message": {
      "type": "string",
      "minLength": 1,
      "maxLength": 2000
    }
  }
}
