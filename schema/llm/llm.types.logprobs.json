{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://adaptersdk.org/schemas/llm/llm.types.logprobs.json",
  "title": "LLM Logprobs",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "tokens": { "type": "array", "items": { "type": "string" }, "maxItems": 8192 },
    "token_logprobs": { "type": "array", "items": { "type": "number" }, "maxItems": 8192 },
    "top_logprobs": {
      "type": "array",
      "items": {
        "type": "array",
        "maxItems": 5,
        "items": {
          "type": "object",
          "additionalProperties": false,
          "required": ["token", "logprob"],
          "properties": {
            "token": { "type": "string" },
            "logprob": { "type": "number" }
          }
        }
      }
    }
  }
}
